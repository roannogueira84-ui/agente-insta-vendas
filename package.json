{
  "name": "app",
  "private": true,
  "scripts": {
    "build": "prisma generate --schema prisma/schema.prisma && prisma db push --schema prisma/schema.prisma && next build",
    "start": "prisma generate --schema prisma/schema.prisma && prisma db push --schema prisma/schema.prisma && next start",
    "dev": "prisma generate --schema prisma/schema.prisma && prisma db push --schema prisma/schema.prisma && next dev",
    "lint": "next lint"
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "tsx --require dotenv/config scripts/seed.ts"
  },
  "devDependencies": {
    "eslint": "8.57.0",
    "@typescript-eslint/parser": "7.18.0",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "eslint-config-next": "15.3.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react-hooks": "4.6.0",
    "@next/swc-wasm-nodejs": "13.5.1",
    "@types/mime-types": "^3.0.1",
    "@types/node": "20.6.2",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "postcss": "8.4.30",
    "prisma": "6.7.0",
    "tailwind-merge": "2.5.2",
    "tailwindcss": "3.3.3",
    "tailwindcss-animate": "1.0.7",
    "ts-node": "10.9.2",
    "tsx": "4.20.3",
    "typescript": "5.2.2"
  },
  "dependencies": {
  "next": "14.2.28",
  "react": "18.2.0",
  "react-dom": "18.2.0",

  "next-auth": "4.24.7",
  "@prisma/client": "6.7.0",
  "nodemailer": "6.9.11",
  "lucide-react": "0.453.0",
  "zod": "3.23.8",
  "axios": "1.7.7",

  "tailwindcss": "3.4.10",
  "postcss": "8.4.47",
  "autoprefixer": "10.4.20",
  "clsx": "2.1.1",
  "class-variance-authority": "0.7.0"
